
(enum "message"
    (constructor "error"
        (arg "stream-id" (int))
        (kwarg "message" optional: #true (string))
    )
    (constructor "open"
        (arg "stream-id" (int))
    )
    (constructor "data"
        (arg "stream-id" (int))
        (arg "data" (esexpr))
    )
    (constructor "close"
        (arg "stream-id" (int))
    )
)
