{
  "plugins": [ "source", "js" ],

  "tube": {
    "loader": {
      "plugin": "source",
      "name": "buildspec"
    },
    "options": {
      "name": ["Argon", "Core"],
      "spec": "Argon.Core.tubespec",
      "sources": "src",
      "plugin": {
        "js": {
          "modules": {
            "Bool": {
              "inject_before": "js/inject_bool_before.js"
            },
            "Int": {
              "inject_before": "js/inject_int_before.js"
            },
            "Puts": {
              "inject_before": "js/inject_puts_before.js"
            },
            "String": {
              "inject_before": "js/inject_string_before.js"
            }
          },
          "extern": [
            "js/extern.js",
            "js/extern_bool.js",
            "js/extern_int.js",
            "js/extern_nat.js",
            "js/extern_puts.js",
            "js/extern_string.js"
          ]
        }
      }
    }
  },

  "output": {
    "js": {
      "package": "bin/js"
    }
  },

  "libraries": []
}