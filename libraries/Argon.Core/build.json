  "plugins": [ "source", "js" ],

  "tube": {
    "loader": {
      "plugin": "source",
      "name": "buildspec"
    },
    "options": {
      "name": ["Argon", "Core"],
      "spec": "Argon.Core.tubespec",
      "sources": "src",
      "plugin": {
        "js": {
          "extern": ["js/extern.js"]
        }
      }
    }
  },

  "output": {
    "js": {
      "package": "bin/js"
    }
  },

  "libraries": []
}